{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <link rel="stylesheet" href="{{ asset("build/form.css") }}"/>

    <div class="container">
        <h2 class="deepshadow">Inscription</h2>

        <div class="login-form bg-light px-3 py-3">
            {{  form_start(form) }}
            {{ form_widget(form) }}
            <button type="submit" class="btn btn-success">S'inscrire</button>
            {{ form_rest(form) }}
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
